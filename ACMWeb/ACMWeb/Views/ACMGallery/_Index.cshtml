@model List<ACM.Core.Models.GalleryViewModel>

<table class="table">
    <thead>
        <tr>
            <th>Gallery</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {

            <tr>
                @*<td> <img src="~/Gallery/@item.Image" /></td>*@
                <td> <img class="myImg" src="~/Thumbnail/@item.ThumbnailImage" onclick='DisplayImageInPopup("/Gallery/@item.Image")' /></td>

            </tr>

        }
    </tbody>
</table>

<div class="modal" id="myModal">
    <div class="modal-dialog">
        <div class="modal-content" style="width:800px!important">

            <!-- Modal Header -->
            <div class="modal-header">
                @*<h4 class="modal-title">Modal Heading</h4>*@
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>

            <!-- Modal body -->
            <div class="modal-body" style="text-align:center;">
                <img class="modal-content" id="img01">
            </div>

            <!-- Modal footer -->
            <div class="modal-footer">
                @*<button onclick="PasswordView();" style="width:100px" data-dismiss="modal">Yes</button>
                <button type="button" class="btn btn-danger" data-dismiss="modal" onclick="cancel()">Cancel</button>*@
            </div>

        </div>
    </div>
</div>
