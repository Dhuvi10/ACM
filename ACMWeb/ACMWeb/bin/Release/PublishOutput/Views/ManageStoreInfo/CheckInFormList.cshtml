@model ACM.Core.Models.AccountViewModels.RegisterStoreViewModel
@{
    ViewData["Title"] = "Index";
    Layout = "_LoggedInLayout";
}
<div class="row">
    @*<a asp-controller="campaign" asp-action="create"><i class="fas fa-plus"></i> Neu Kampagne</a>*@
    <div class="col-xs-12">
        <div class="x_panel">
            <div class="x_title">
                @*<h2>Verwalten Kampagnen</h2>*@
                <div class="clearfix"></div>
            </div>
            <div class="x_content">
                @*@if (Model.Count == 0)
                    {
                        <h3>Keine Kampagne gefunden!</h3>
                    }*@
                <div id="divList">
                </div>

            </div>
        </div>
    </div>
</div>
@section Scripts
    {
<script type="text/javascript">
   $(function()
    {
       CheckinList();
    });
    function CheckinList() {
        $.ajax({
            method: "get",
            url: "@Url.Action("PartialList", "ManageStoreInfo")",
            success: function (result) {
                debugger
                console.log(result);
            $('#divList').html(result);
            },
            error: function (err) {
            console.log(err);
            }
            });
    }
</script>
        }