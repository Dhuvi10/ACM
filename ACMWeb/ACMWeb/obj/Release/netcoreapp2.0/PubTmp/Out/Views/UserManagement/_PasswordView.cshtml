
<div class="signin-up-area">
    <div class="form-cont">
        <div class="form">
            <form asp-route-returnUrl="@ViewData["ReturnUrl"]" method="post">
                @*<div asp-validation-summary="All" class="text-danger"></div>*@
                <h1 class="h3 mb-5 font-weight-bold"><span> @ViewData["Title"]</span></h1>
                <input id="txtid" name="id" type="hidden"  class="form-control" value="@ViewData["id"]" />
                <input id="txtstatus" name="status"  type="hidden"  class="form-control" value="@ViewData["status"]" />
                <div class="form-group">
                    <label>Password</label>
                    <input name="Password" id="txtpassword" type="password" class="form-control" required />
                    
                </div>

                <button type="button" onclick="StatusChangedPopup();" style="width:100px">Save</button>
                <a href="@Url.Action("Index","UserManagement")" class="btn btn-default">Cancel</a>
            </form>
        </div>
    </div>
</div>
<div class="modal" id="myModal">
    <div class="modal-dialog">
        <div class="modal-content">

            <!-- Modal Header -->
            <div class="modal-header">
                @*<h4 class="modal-title">Modal Heading</h4>*@
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>

            <!-- Modal body -->
            <div class="modal-body">
                The password entered is incorrect Try Again
            </div>

            <!-- Modal footer -->
            <div class="modal-footer">
                 <button id="btnStatuslock" onclick="StatusChanged()" style="width:100px" data-dismiss="modal">Yes</button>
                <button onclick="passwordTryAgain();" style="width:100px;display:none" data-dismiss="modal" id="btnTryagain">Yes</button>
                <button type="button" class="btn btn-danger" data-dismiss="modal" onclick="cancel()">Cancel</button>
            </div>

        </div>
    </div>
</div>
